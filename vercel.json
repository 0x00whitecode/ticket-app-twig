{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "@vercel/php@0.6.0"
    }
  },
  "builds": [
    {
      "src": "index.php",
      "use": "@vercel/php@0.6.0"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false"
  }
}
